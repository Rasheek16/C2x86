   .globl foo
   .text
foo:
   pushq   %rbp
   movq    %rsp, %rbp
   subq $16, %rsp
   movl $1, -8(%rbp)
   .globl main
   .text
main:
   pushq   %rbp
   movq    %rsp, %rbp
   subq $16, %rsp
   call foo
   movl %eax, -8(%rbp)
   movl $3, %eax
   movq   %rbp, %rsp
   popq   %rbp
   ret
   movl $0, %eax
   movq   %rbp, %rsp
   popq   %rbp
   ret
.section .note.GNU-stack,"",@progbits
