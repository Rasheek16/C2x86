   .globl main
   .text
main:
   pushq   %rbp
   movq    %rsp, %rbp
   subq    $0, %rsp
   movl foo(%rip), %eax
   movq   %rbp, %rsp
   popq   %rbp
   ret
   movl $0, %eax
   movq   %rbp, %rsp
   popq   %rbp
   ret
   .globl foo
   .bss
   .align 4
foo:
   .zero 4
.section .note.GNU-stack,"",@progbits
